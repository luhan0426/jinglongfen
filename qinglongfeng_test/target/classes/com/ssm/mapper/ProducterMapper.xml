<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.mapper.ProducterMapper">
	<select id="retrieveList" resultType="producter">
		select * from producter
	</select>
	<insert id="toLiuyan">
		insert into liuyan (proname,dname,username,tel,qq) values (#{proname},#{dname},#{username},#{tel},#{qq})
	</insert>
</mapper>